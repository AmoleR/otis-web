# [START django_app]
runtime: python27
api_version: 1
threadsafe: yes
env: standard

handlers:
  - url: /static
    static_dir: static/
  - url: .*
    script: otisweb.wsgi.application

instance_class: B1
basic_scaling:
  max_instances: 2
  idle_timeout: 10m

beta_settings:
  cloud_sql_instances: <fill-in>

libraries:
  - name: MySQLdb
    version: "latest"
  - name: django
    version: "1.11"

skip_files:
  - ^(.*/)?.*\.py[co]$
  - ^\.git/.*$

env_variables:
  DATABASE_NAME: <fill-in>
  DATABASE_USER: <fill-in>
  DATABASE_PASSWORD: <fill-in>
  DATABASE_HOST: <fill-in>
  STATIC_URL: <fill-in>
  GS_ACCESS_KEY_ID: <fill-in>
  GS_SECRET_ACCESS_KEY: <fill-in>
  GS_BUCKET_NAME: <fill-in>
  MEDIA_URL: <fill-in>
  IS_PRODUCTION: 1

# [END django_app]
